				{
					"type": "host"
				},
				{
					"type": "os"
				},
				{
					"type": "kernel"
				},
				{
					"type": "uptime"
				},
				{
					"type": "shell"
				},
				{
					"type": "packages"
				},
				{
					"type": "memory",
					"compactType": true
				},
				{
					"type": "colors"
				}
			],
			"colors": {
				"separator": " â”‚ ",
				"colors": [
					"red",
					"orange",
					"yellow",
					"green",
					"cyan",
					"blue",
					"magenta",
